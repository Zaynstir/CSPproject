{
  "manifest_version": 2,

  "name": "ForMal",
  "description": "This does fun stuff",
  "version": "9999.9",

  "browser_action":{
  	  "default_popup": "popup.html"
  },
  
  "background" :  {
	  "page" : "popup.js"
  },
  
  "permissions": [
   "activeTab",
    "identity",
    "input",
    "privacy",
    "webRequest",
          "*://*.google.com/"
   ],
   "content_scripts": [
   		{
	  	 	"matches" : ["<all_urls>"],
	  	 	"js": ["jquery-3.2.0.min.js","SwapAndChange.js"],
			"run_at" : "document_end"
			
   		}
   ]
  
}